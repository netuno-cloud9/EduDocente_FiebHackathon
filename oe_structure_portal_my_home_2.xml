 <!-- Custom Cursor Script -->
       <!-- Custom Cursor Script -->
<script>
    document.addEventListener('DOMContentLoaded', function () {
        // Cria o elemento de cursor personalizado
        const cursor = document.createElement('div');
        cursor.classList.add('custom-cursor');
        document.body.appendChild(cursor);

        // Adiciona o estilo do cursor ao CSS
        const style = document.createElement('style');
        style.textContent = `
            .custom-cursor {
                width: 20px;
                height: 20px;
                border-radius: 50%;
                background-color: #1c1c21; /* Cor padrão do cursor */
                position: absolute;
                pointer-events: none;
                transition: background-color 0.3s ease, transform 0.1s ease;
                z-index: 10000;
                opacity: 0.8;
            }
        `;
        document.head.appendChild(style);

        // Movimento do cursor personalizado
        document.addEventListener('mousemove', (e) =&gt; {
            cursor.style.left = `${e.pageX}px`;
            cursor.style.top = `${e.pageY}px`;
        });

        // Muda a cor do cursor ao entrar em uma seção com atributo data-cursor-color
        const sections = document.querySelectorAll('[data-cursor-color]');
        sections.forEach((section) =&gt; {
            section.addEventListener('mouseenter', () =&gt; {
                const color = section.getAttribute('data-cursor-color');
                cursor.style.backgroundColor = color;
            });

            section.addEventListener('mouseleave', () =&gt; {
                cursor.style.backgroundColor = '#1c1c21'; // Cor padrão do cursor
            });
        });
    });
</script>

